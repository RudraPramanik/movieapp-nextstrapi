{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "name": "Movies"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "default": "movie name"
    },
    "release_date": {
      "type": "date"
    },
    "poster": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "actors": {
      "via": "movies",
      "collection": "actors"
    }
  }
}
